import java.util.Arrays;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
      Person user = new Person();
      Scanner scan = new Scanner(System.in);
      user.login = scan.next();
      user.parol = scan.nextInt();
//      user.login = "aaafffa1";
//      user.parol = 1234;
      user.displayInfo();
    }

}
class Person{
String login;
int parol;
void displayInfo(){
    System.out.printf("Login: %s\t Parol: %d\n", login, parol);
}
}


